package lista01;
import java.util.Scanner;
public class Q05 {
	public static void main(String[] args) {
		Scanner scr = new Scanner (System.in);
		int dias,meses,anos,total;
		dias=scr.nextInt();
		meses=scr.nextInt();
		anos=scr.nextInt();
		total=dias+meses*30+anos*365;
		System.out.println("A pessoa viveu "+total+" dias de nascida");
	}

}
